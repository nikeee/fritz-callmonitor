{
  "compilerOptions": {
    "module": "nodenext",
    "target": "es2015",
    "lib": [
      "es2015"
    ],
    "outDir": "lib",
    "declaration": true,
    "sourceMap": false,
    "newLine": "LF",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "alwaysStrict": true
  }
}
